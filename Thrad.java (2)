
 class Thrad extends Thread
 {
    void display()
    {
    Thread g=new currentThread();
    for (i=0;i<=10;i++)
    {
        try
      {
      g.sleep(1000);
      System.out.println(i);
      }
     catch(Exception e)
     {
      }
    }
   }
}
public class Tthread
{
    public static void main(String args)
    {
        Thrad t=new Thrad();
        t.start();
    }
}
    
 
