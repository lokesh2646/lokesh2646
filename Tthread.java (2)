
 class Thrad extends Thread
 {
    void display()
    {
    Thread g=currentThread();
    for (int i=0;i<=10;i++)
    {
        try
      {
      Thread.sleep(1000);
      }
     catch(Exception e)
     {
      }
      System.out.println(g.getName()+""+i);
    }
   }
}
public class Tthread
{
    public static void main(String args [])
    {
        Thrad t1=new Thrad();
        Thrad t2=new Thrad();
        t1.start();
        t2.start();
        
    }
}
    
 
